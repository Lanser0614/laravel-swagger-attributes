{
    "name": "bellissimopizza/laravel-swagger-attributes",
    "description": "Laravel OpenApi documentation generator using PHP attributes",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "BellissimoPizza\\SwaggerAttributes\\": "src/"
        }
    },
    "authors": [
        {
            "name": "BellissimoPizza"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0",
        "swagger-api/swagger-ui": "^4.0",
        "symfony/yaml": "^5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "BellissimoPizza\\SwaggerAttributes\\Providers\\SwaggerAttributesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
